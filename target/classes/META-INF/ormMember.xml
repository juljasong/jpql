<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm" version="2.2">
    <named-query name="Member.findByUsername">
        <query><![CDATA[
        SELECT m
        FROM Member m
        WHERE m.username = :username
        ]]></query>
    </named-query>
    <named-query name="Member.count">
        <query>
            SELECT COUNT(m)
            FROM Member m
        </query>
    </named-query>
</entity-mappings>